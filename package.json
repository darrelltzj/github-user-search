{
  "name": "github-user-search",
  "version": "1.0.0",
  "main": "src/server.js",
  "repository": "https://github.com/darrelltzj/github-user-search",
  "author": "darrelltzj",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "concurrently": "^4.1.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "nodemon": "^1.18.7",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "axios": "^0.18.0",
    "babel-minify-webpack-plugin": "^0.3.1",
    "brotli-webpack-plugin": "^1.0.0",
    "compression-webpack-plugin": "^2.0.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-static-gzip": "^1.1.3",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.0",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "1.x",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "serialize-javascript": "^1.5.0",
    "styled-components": "^4.1.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.27.0",
    "webpack-merge": "^4.1.4"
  },
  "scripts": {
    "start": "node build/server.js",
    "build": "concurrently \"yarn build:server\"  \"yarn build:client\"",
    "build:server": "babel src --out-dir build",
    "build:client": "webpack --mode production --config=config/webpack.prod.js",
    "clean": "concurrently \"yarn clean:server\"  \"yarn clean:client\"",
    "clean:server": "rm -rf build && mkdir build",
    "clean:client": "rm -rf public && mkdir public",
    "dev": "concurrently \"yarn dev:server\"  \"yarn dev:client\"",
    "dev:server": "nodemon --exec babel-node src/server.js",
    "dev:client": "webpack --mode development --config=config/webpack.dev.js --watch",
    "lint": "eslint 'src/**/*.js'",
    "lint:fix": "eslint 'src/**/*.js' --fix",
    "heroku-postbuild": "yarn clean && yarn build"
  },
  "engines": {
    "node": "10.14.1",
    "npm": "6.4.1",
    "yarn": "1.12.3"
  }
}
